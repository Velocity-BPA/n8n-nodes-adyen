{
  "name": "n8n-nodes-adyen",
  "version": "1.0.0",
  "description": "n8n community node for Adyen enterprise payment processing with checkout sessions, modifications, disputes, and webhooks",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-adyen.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-adyen",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-adyen/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "adyen",
    "payments",
    "payment-processing",
    "checkout",
    "psp",
    "payment-gateway",
    "ecommerce",
    "recurring-payments",
    "disputes",
    "chargebacks",
    "webhooks"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AdyenApi.credentials.js"
